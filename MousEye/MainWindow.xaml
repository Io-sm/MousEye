<Window x:Class="MousEye.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:MousEye"
    Title="CLEyeMulticamWPFTest" Height="540" Width="800" Background="Gray">
    <Grid>
        <local:CameraImage x:Name="cameraImage1" Margin="10,10,0,0" Width="640" Height="480" HorizontalAlignment="Left"  VerticalAlignment="Top" Framerate="30" ColorMode="CLEYE_COLOR_PROCESSED" Resolution="CLEYE_VGA" />
        <local:CameraImage x:Name="cameraImage2" Margin="10,260,0,0" Width="320" Height="240" HorizontalAlignment="Left"  VerticalAlignment="Top" Framerate="30" ColorMode="CLEYE_COLOR_PROCESSED" Resolution="CLEYE_QVGA" />
        <ListBox Name="output" Height="240" Margin="677,10,10,0" VerticalAlignment="Top" IsTextSearchEnabled="False" IsHitTestVisible="False" Background="LightGray" />
    </Grid>
</Window>