<Window x:Class="MousEye.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:MousEye.Views"
        xmlns:tb="http://www.hardcodet.net/taskbar"
        Title="MousEye" Height="700" Width="800" Background="Gray" ResizeMode="NoResize"
        Visibility="Visible">

    <Window.Content>
        <Grid>
            <tb:TaskbarIcon IconSource="/Icons/mouseye.ico" ToolTipText="kupa" TrayLeftMouseDown="TaskbarIcon_OnTrayLeftMouseDown" />
            <local:CameraView />
        </Grid>
    </Window.Content>
</Window>